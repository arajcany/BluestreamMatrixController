{
  "name": "arajcany/blustream-matrix-controlelr",
  "description": "PHP Library to control Blustream Matrix Switchers via HTTP-CGI Requests.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.2",
    "ext-dom": "*",
    "cakephp/utility": "^4.2",
    "ext-simplexml": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "ext-simplexml": "*"
  },
  "autoload": {
    "psr-4": {
      "arajcany\\BlustreamMatrixController\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "arajcany\\tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "prefer-stable": true
}
